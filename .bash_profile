export ANDROID_HOME=/Users/dong/Library/Android/sdk
export ANDROID_SDK_ROOT=/Users/dong/Library/Android/sdk
export GRADLE_HOME=/Users/dong/gradle/gradle-4.6
export PATH=${PATH}:${ANDROID_HOME}/tools
export PATH=${PATH}:${ANDROID_HOME}/platform-tools
export PATH=$PATH:$GRADLE_HOME/bin:

export PATH=/Users/dong/Project/repo/flutter/bin:$PATH
export PUB_HOSTED_URL=https://pub.flutter-io.cn
export FLUTTER_STORAGE_BASE_URL=https://storage.flutter-io.cn

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
